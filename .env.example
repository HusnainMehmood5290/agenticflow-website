# Environment Variables (Next.js requires NEXT_PUBLIC_ prefix for client-side vars)

# Site Configuration
NEXT_PUBLIC_SITE_URL=https://agenticflow.me
NEXT_PUBLIC_SITE_NAME=AgenticFlow

# Analytics
NEXT_PUBLIC_GA_MEASUREMENT_ID=G-XXXXXXXXXX
NEXT_PUBLIC_GTM_ID=GTM-XXXXXXX

# Contact Email
NEXT_PUBLIC_CONTACT_EMAIL=contact@agenticflow.me

# Feature Flags
NEXT_PUBLIC_ENABLE_ANALYTICS=true
NEXT_PUBLIC_ENABLE_CHATBOT=false
